{
    "name": "itsdamien/laravel-model-transformer",
    "description": "A simple model transformer compatible with Laravel 5+.",
    "keywords": [
        "laravel",
        "api",
        "json",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Criado",
            "email": "its@damien.is"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "illuminate/database": "~5.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ItsDamien\\Transformer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ItsDamien\\Transformer\\Tests\\": "tests/"
        }
    }
}
